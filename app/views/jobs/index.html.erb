<div class="row">
  <div class="col-sm-12 well">
    <h2 class="center">Your job prospects:</h2>
    <div class="row job-list" id="heading">
      <div class="col-sm-3 job-list-title">
        <h3>Company</h3>
      </div>
      <div class="col-sm-3 job-list-title">
        <h3>Job Title</h3>
      </div>
      <div class="col-sm-3 job-list-title">
        <h3>Posting Date</h3>
      </div>
      <div class="col-sm-3 job-list-title">
        <h3>Closing Date</h3>
      </div>
    </div>
    <div class="jobs">
      <%= render 'job' %>
    </div>
  </div>
</div>
