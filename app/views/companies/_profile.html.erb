<h2 class="center">Profile</h2>
  <h4 class= "center"><%= link_to "Company Website", company.response['website'], target: "_blank" %></h4>
  <hr>
  <p class="center"><%= company.response['socialProfiles'][1]['bio'] %></p>
  <hr>
  <h3 class="center">Social Media</h3>
  <div class="row">
    <% company.response['socialProfiles'].each do |profile| %>
        <div class="col-xs-3" id="social-buttons">
          <p class="center"><%=link_to profile['typeName'], profile['url'], target: "_blank", class: "btn btn-warning"%></p>
        </div>
    <% end %>
  </div>
<hr>
  <h3 class="center">Contacts</h3>
  <ul>
    <% @company.contacts.each do |contact| %>
      <li><%= link_to contact.last_name+', '+contact.first_name, contact_path(contact) %></li>
    <% end %>
  </ul>
<hr>
  <h3 class="center">Jobs</h3>
  <ul>
    <% @jobs.each do |job| %>
      <li><%= link_to job.title.titleize, company_job_path(@company, job) %></li>
    <% end %>
  </ul>
