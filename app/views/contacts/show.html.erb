<div class="row">
  <h1><%= @contact.first_name %> <%= @contact.last_name %></h1>

  <p>  <%= link_to "Edit", edit_contact_path(@contact), :class => "btn btn-warning" %>
  <%= link_to 'Delete', contact_path(@contact), :data => {:confirm => "Are you sure?", :method => "delete"}, :class => "btn btn-danger" %></p>
</div>
